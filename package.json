{
  "name": "dvbjs",
  "version": "5.0.0",
  "description": "query Dresden's public transport system for current bus- and tramstop data",
  "main": "lib/index.js",
  "typings": "js/index.d.ts",
  "files": [
    "lib/",
    "index.js"
  ],
  "scripts": {
    "build": "tsc -p .",
    "clean": "rimraf coverage lib",
    "test": "NODE_ENV=test mocha",
    "test-live": "NODE_ENV=test_live mocha",
    "update-mocks": "NODE_ENV=test_update mocha",
    "coverage": "NODE_ENV=test node node_modules/.bin/istanbul cover _mocha -- -R spec",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "lint": "tslint {src,test}/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kiliankoe/dvbjs.git"
  },
